# =========================
# call_me plugin gitignore
# =========================

# Python cache
__pycache__/
*.py[cod]
*$py.class

# Local runtime/config
call_me.db
config.toml
config.toml.backup
*.log

# Local model/data artifacts
asr/
*.tar.bz2
*.zip
*.onnx
*.ckpt
*.pth
*.safetensors
*.wav

# Frontend local artifacts
frontend/node_modules/
frontend/.vite/
frontend/.vite-temp/
frontend/test-results/
frontend/playwright-report/
frontend/coverage/
frontend/tsconfig.tsbuildinfo
frontend/.env.local
frontend/.env.*.local

# Legacy or local-only test locations
test/
tests/
frontend/tests-e2e/

# Local editor/tool files
.idea/
.vscode/
.cursorignore
.cursorindexingignore

# Nested git metadata (if this plugin directory was copied from another repo)
.git/
